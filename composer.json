{
  "name": "t3g/contentimporter",
  "description": "Create content from Microsoft Office Docs",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "repositories": {
    "0": {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  },
  "authors": [
    {
      "name": "TYPO3 GmbH",
      "role": "Developer",
      "homepage": "https://typo3.com/"
    }
  ],
  "config": {
    "sort-packages": true,
    "discard-changes": true,
    "vendor-dir": ".build/vendor",
    "bin-dir": ".build/bin"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "contentimporter",
      "app-dir": ".build",
      "web-dir": ".build/public"
    },
    "branch-alias": {
      "dev-develop": "1.0.x-dev"
    }
  },
  "scripts": {
    "t3g:phplint": [
      "phplint"
    ],
    "t3g:cgl": [
      "php-cs-fixer fix -v --dry-run --using-cache false --diff --diff-format=udiff"
    ]
  },
  "autoload": {
    "psr-4": {
      "T3G\\Contentimporter\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "^9.5",
    "phpoffice/phpword": "^0.13.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "overtrue/phplint": "^1.1",
    "phpunit/phpunit": "*"
  }
}
